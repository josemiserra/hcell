<PIPELINE>
	<MODULE seq="001" name="FILE PROCESSING">
		<FUNCTION name="LOAD FILE">
			<REGEXP>nuclei.jpg</REGEXP>
			<OUTPUT>IMAGE1</OUTPUT>
		</FUNCTION>
	</MODULE>
