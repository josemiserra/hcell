			<INPUT>NUC_END</INPUT>
			<OUTPUT>NUC_OBJ</OUTPUT>
		</FUNCTION>
	</MODULE> -->
	<MODULE seq="007" name="SEGMENTATION PROCESSING">
		<FUNCTION name="SPLIT BY WATERSHED">
			<INPUT>NUC_END</INPUT>
			<REFERENCE>IMAGE1</REFERENCE>
			<METHOD>INTENSITY</METHOD>
			<EXT>1</EXT>
			<TOL>1.0</TOL>
			<SAVE_OBJ>NUC_OBJ</SAVE_OBJ>
		</FUNCTION>
	</MODULE>
	<MODULE seq="007" name="FILE PROCESSING">
		<FUNCTION name="PAINT OBJECTS">
			<ASHOW>FALSE</ASHOW>
			<WINDOWSNAME>Painted </WINDOWSNAME>
			<INPUT>IMAGE1</INPUT>
			<COLOUR>RANDOM</COLOUR>
			<THICKNESS>0</THICKNESS>
			<LOAD_OBJECTS>NUC_OBJ</LOAD_OBJECTS>
			<PAINT_CONTOURS>TRUE</PAINT_CONTOURS>
			<OUTPUT>IM_1</OUTPUT>
			<WIDTH>800</WIDTH>
			<HEIGHT>600</HEIGHT>
		</FUNCTION>
		</MODULE>
	<MODULE seq="008" name="COMPUTE FEATURES">
		<FUNCTION name="BASIC">
			<LOAD_OBJECTS>NUC_OBJ</LOAD_OBJECTS>
			<REFERENCE>IMAGE1</REFERENCE>
			<MEAN>TRUE</MEAN>
			<SD>TRUE</SD>
			<MAD>TRUE</MAD>
			<QUANTILES>TRUE</QUANTILES>
			<SAVE_INDIVIDUAL>TRUE</SAVE_INDIVIDUAL>
			<FOUTPUT>FEAT_BASIC</FOUTPUT>
			<APPEND_ORIGINAL_NAME>FALSE</APPEND_ORIGINAL_NAME>
			<APPEND_DATE>TRUE</APPEND_DATE>
		</FUNCTION>
	</MODULE> 
	<MODULE seq="009" name="COMPUTE FEATURES">
		<FUNCTION name="SHAPE">
			<LOAD_OBJECTS>NUC_OBJ</LOAD_OBJECTS>
			<REFERENCE>IMAGE1</REFERENCE>
			<AREA>TRUE</AREA>
			<PERIMETER>TRUE</PERIMETER>
			<RADIUS>TRUE</RADIUS>
			<ROUNDNESS>TRUE</ROUNDNESS>
			<SAVE_INDIVIDUAL>TRUE</SAVE_INDIVIDUAL>
			<FOUTPUT>FEAT_BASIC</FOUTPUT>
			<APPEND_ORIGINAL_NAME>FALSE</APPEND_ORIGINAL_NAME>
			<APPEND_DATE>TRUE</APPEND_DATE>
		</FUNCTION>
	</MODULE> 
	<MODULE seq="017" name="COMPUTE FEATURES">
		<FUNCTION name="MOMENT">
			<LOAD_OBJECTS>NUC_OBJ</LOAD_OBJECTS>
			<REFERENCE>IMAGE1</REFERENCE>
			<CENTROID>TRUE</CENTROID>
			<AXIS>TRUE</AXIS>
			<ECCENTRICITY>TRUE</ECCENTRICITY>
			<THETA>TRUE</THETA>
			<SAVE_INDIVIDUAL>TRUE</SAVE_INDIVIDUAL>
			<FOUTPUT>FEAT_BASIC</FOUTPUT>
			<APPEND_ORIGINAL_NAME>FALSE</APPEND_ORIGINAL_NAME>
			<APPEND_DATE>TRUE</APPEND_DATE>
		</FUNCTION>
	</MODULE> 
	<MODULE seq="018" name="COMPUTE FEATURES">
		<FUNCTION name="HARALICK">
		 <!-- Haralick features can be choosen -->
		 <!--ASM, CON, COR, VAR, IDM, SAV, SVA, SEN, ENT, DVA, DEN, IMC1, IMC2 -->			
			<LOAD_OBJECTS>NUC_OBJ</LOAD_OBJECTS>
			<REFERENCE>IMAGE1</REFERENCE>
			<!-- Usually 8 for 8 bits, 16 for 16 bits, and so on-->
			<BINS>8</BINS>
			<!-- List of comma separated values for scale distance -->
			<SCALES>1,2</SCALES>
			<FOUTPUT>FEAT_HARALICK</FOUTPUT>
			<APPEND_ORIGINAL_NAME>FALSE</APPEND_ORIGINAL_NAME>
			<APPEND_DATE>TRUE</APPEND_DATE>
		</FUNCTION>
	</MODULE> 
</PIPELINE>
