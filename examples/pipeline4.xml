<PIPELINE>
	<MODULE seq="001" name="FILE PROCESSING">
		<FUNCTION name="LOAD FILE">
			<REGEXP>tape.jpeg</REGEXP>
			<OUTPUT>IMAGE1</OUTPUT>
		</FUNCTION>
	</MODULE>
	<MODULE seq="003" name="FILE PROCESSING">
		<FUNCTION name="SHOW">
			<INPUT>IMAGE1</INPUT>
			<WINDOWSNAME>tape</WINDOWSNAME>
			<WIDTH>800</WIDTH>
			<HEIGHT>600</HEIGHT>
			<TYPE>WINDOW</TYPE>
		</FUNCTION>
	</MODULE> 
	<MODULE seq="002" name="FILE PROCESSING">
		<FUNCTION name="NORMALIZE">
			<INPUT>IMAGE1</INPUT>
			<OUTPUT>IMAGE12</OUTPUT>
			<ASHOW>TRUE</ASHOW>
			<WINDOWSNAME>NORM</WINDOWSNAME>
			<!-- MAXMIN or LOG-->
			<TYPE>LOG</TYPE>
			<MAXINT>1</MAXINT>
			<MININT>0</MININT>
		</FUNCTION>
	</MODULE>
	<MODULE seq="002" name="IMAGE PROCESSING">
		<FUNCTION name="THRESHOLD">
			<INPUT>IMAGE12</INPUT>
			<THRESHOLD>0.75</THRESHOLD>
			<THRESHOLD_TYPE>THRESH_BINARY_INV</THRESHOLD_TYPE>
			<MAX_THRESHOLD>1</MAX_THRESHOLD>
			<ASHOW>TRUE</ASHOW>
			<WINDOWSNAME>THRESHOLD</WINDOWSNAME>
			<OUTPUT>IMAGE2</OUTPUT>
		</FUNCTION>
	</MODULE>
	<MODULE seq="004" name="IMAGE PROCESSING">
		<FUNCTION name="FILLHULL">
			<ASHOW>TRUE</ASHOW>
			<WINDOWSNAME>FILLED</WINDOWSNAME>
			<INPUT>IMAGE2</INPUT>
			<OUTPUT>IMAGE3</OUTPUT>
		</FUNCTION>
	</MODULE>
	<MODULE seq="005" name="IMAGE PROCESSING">
		<FUNCTION name="CANNY">
			<ASHOW>TRUE</ASHOW>
			<WINDOWSNAME>CANNY EDGES</WINDOWSNAME>
			<HIGH_THRESHOLD>100</HIGH_THRESHOLD>
			<LOW_THRESHOLD>10</LOW_THRESHOLD>
			<L2_GRADIENT>FALSE</L2_GRADIENT>
			<KERNEL_SIZE>5</KERNEL_SIZE>
			<INPUT>IMAGE3</INPUT>
			<OUTPUT>IMAGE4</OUTPUT>
		</FUNCTION>
	</MODULE>
</PIPELINE>
